{
    "name": "filipedtristao/laravel-crud-builder",
    "authors": [
        {
            "name": "Filipe Trist√£o",
            "email": "filipedtristao@gmail.com",
			"role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.6.34|~5.7.0|~5.8.0|^6.0",
        "illuminate/http": "~5.6.34|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0",
        "ext-json": "*"
    },
	"autoload": {
        "psr-4": {
            "CrudBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrudBuilder\\Tests\\": "tests"
        }
    },
	"scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
